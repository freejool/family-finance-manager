<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"

    package="com.example.as">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@color/backgroundColor"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">

        <activity
            android:name="com.example.as.activity.Login"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.as.activity.MainActivity"
            android:label="@string/app_name"></activity>
        <activity
            android:name="com.example.as.activity.AddOutaccount"
            android:label="@string/addoutaccount"></activity>
        <activity
            android:name="com.example.as.activity.AddInaccount"
            android:label="@string/addinaccount"></activity>
        <activity
            android:name="com.example.as.activity.Outaccountinfo"
            android:label="@string/outaccountinfo"></activity>
        <activity
            android:name="com.example.as.activity.Inaccountinfo"
            android:label="@string/inaccountinfo"></activity>
        <activity
            android:name="com.example.as.activity.Showinfo"
            android:label="@string/showinfo"></activity>
        <activity
            android:name="com.example.as.activity.Sysset"
            android:label="@string/sysset"></activity>
        <activity
            android:name="com.example.as.activity.Accountflag"
            android:label="@string/accountflag"></activity>
        <activity
            android:name="com.example.as.activity.InfoManage"
            android:label="@string/infomanage"></activity>
        <activity
            android:name="com.example.as.activity.FlagManage"
            android:label="@string/flagmanage"></activity>
        <activity
            android:name="com.example.as.activity.Help"
            android:label="@string/help"></activity>
        <activity
            android:name="com.example.as.activity.TotalChart"
            android:label="@string/chart"></activity>
    </application>

</manifest>